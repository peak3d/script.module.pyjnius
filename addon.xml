<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.module.pyjnius"
       name="pyjnius"
       version="1.1.1"
       provider-name="kivy">
  <requires>
    <import addon="xbmc.python" version="2.7.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en_GB">A Python module to access Java classes as Python classes using JNI.</summary>
    <description lang="en_GB">Packed for KODI from https://github.com/kivy/pyjnius</description>
    <platform>android</platform>
    <language></language>
    <license>MIT</license>
    <forum></forum>
    <website>https://pyjnius.readthedocs.io/en/latest/</website>
    <source>https://github.com/kivy/pyjnius</source>
    <email>kivy-dev AT googlegroups DOT com</email>
  </extension>
</addon>
